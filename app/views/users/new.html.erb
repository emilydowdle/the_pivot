<h2>Create an Account</h2>
<%= form_for @user, html: { multipart: true } do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :avatar %>
  <%= f.file_field :avatar %> 
  <%= f.submit "Create Account" %>
<% end %>
