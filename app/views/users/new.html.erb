<h2>Create an Account</h2>
<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username%>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :bio %>
  <%= f.text_field :bio %>
  <%= f.label :image, "Attach an Image" %><br>
  <%= f.file_field :image %><br><br>
  <%= f.submit "Create Account" %>
<% end %>
